<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

  <declare-styleable name="ZoomLinearLayout">
    <attr format="integer" name="maxRotateDegree"/>
    <attr format="integer" name="maxDepth"/>
  </declare-styleable>

  <declare-styleable name="MaskLayout">
    <attr format="reference" name="foreground"/>
    <attr format="dimension" name="content_margin"/>
